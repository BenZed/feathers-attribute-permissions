{
  "name": "feathers-attribute-permissions",
  "version": "0.1.0",
  "description": "A versioning service for feathers.js",
  "main": "lib",
  "scripts": {
    "test": "mocha --require babel-register --require babel-polyfill 'test/*test.js'",
    "test:watch": "mocha --require babel-register --watch --require babel-polyfill 'test/**/*test.js'",
    "build": "rm -rf lib; mkdir lib; node_modules/.bin/babel src --out-dir lib --plugins transform-runtime",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenZed/feathers-attribute-permissions.git"
  },
  "keywords": [
    "feathers",
    "attribute",
    "permissions"
  ],
  "author": "Ben Gaumond",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BenZed/feathers-attribute-permissions/issues"
  },
  "homepage": "https://github.com/BenZed/feathers-attribute-permissions#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "cli-clear": "^1.0.4",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "feathers": "^2.1.3",
    "feathers-authentication": "^1.2.0",
    "feathers-authentication-jwt": "^0.3.1",
    "feathers-authentication-local": "^0.3.4",
    "feathers-configuration": "^0.4.1",
    "feathers-hooks": "^2.0.1",
    "feathers-memory": "^1.1.0",
    "feathers-rest": "^1.7.3",
    "isomorphic-fetch": "^2.2.1",
    "localstorage-memory": "^1.0.2",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "feathers-errors": "^2.8.1",
    "is-explicit": "^1.1.0"
  }
}
